<?xml version="1.0" encoding="utf-8"?>
<odoo>
		<data>
				<record model="ir.ui.view" id="haoyi_city_form">
						<field name="name">haoyi.city.form</field>
						<field name="model">haoyi.city</field>
						<field name="type">form</field>
						<field name="arch" type="xml">
								<form string="city">
										<group>
												<field name="name"/>
												<field name="state"/>
										</group>
								</form>
						</field>
				</record>
				<record model="ir.ui.view" id="haoyi_city_tree">
						<field name="name">haoyi.city.tree</field>
						<field name="model">haoyi.city</field>
						<field name="type">tree</field>
						<field name="arch" type="xml">
								<tree string="city">
										<field name="name"/>
								</tree>
						</field>
				</record>
				<record model="ir.ui.view" id="haoyi_district_form">
						<field name="name">haoyi.district.form</field>
						<field name="model">haoyi.district</field>
						<field name="type">form</field>
						<field name="arch" type="xml">
								<form string="district">
										<group>
												<field name="name"/>
												<field name="city"/>
										</group>
								</form>
						</field>
				</record>
		</data>
</odoo>
